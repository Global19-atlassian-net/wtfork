{
  "name": "wtfork",
  "description": "Strip away the complexity of handling interprocess communication of your forked child processes - allows event namespacing between parent and child processes.",
  "version": "0.1.0",
  "main": "lib/index.js",
  "author": "salakar",
  "repository": {
    "type": "git",
    "url": "git://github.com/salakar/wtfork.git"
  },
  "license": "MIT",
  "scripts": {
    "compile": "babel src --out-dir lib",
    "example": "babel-node example/parent",
    "compile-watch": "babel src --out-dir lib --watch src",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "prepublish": "npm run compile",
    "test": "babel-node ./node_modules/.bin/isparta cover _mocha"
  },
  "devDependencies": {
    "babel-cli": "*",
    "babel-preset-es2015-node4": "*",
  },
  "dependencies": {
    "cuid": "^1.3.8"
  }
}
